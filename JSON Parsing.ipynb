    "import json\n",
    "import urllib.request, urllib.error, urllib.response , urllib.parse\n",
    "\n",
    "url = input(\"Enter URL: \")\n",
    "uh = urllib.request.urlopen(url)\n",
    "data = uh.read().decode()\n",
    "mSum = 0\n",
    "js = json.loads(data)\n",
    "for i in js[\"comments\"]:\n",
    "    mSum = mSum + int(i[\"count\"])\n",
    "\n",
    "print(mSum)"
   ]
